{
  "ConnectionStrings": {
    "WWIDatabase": "Server=tcp:<servername>.database.windows.net,1433;Initial Catalog=WideWorldImporters-Full;User ID=<username>;Password=<password>;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "[InstrumentationKey]"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
